<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<property-set xmlns="http://tapaal.net/">
  
  <property>
    <id>First Query Ever</id>
    <description>First Query Ever</description>
    <formula>
      <exists-path>
        <finally>
          <conjunction>
            <integer-eq>
              <tokens-count>
                <place>P0</place>
              </tokens-count>
              <integer-constant>10</integer-constant>
            </integer-eq>
            <integer-eq>
              <tokens-count>
                <place>P4</place>
              </tokens-count>
              <integer-constant>4</integer-constant>
            </integer-eq>
          </conjunction>
        </finally>
      </exists-path>
    </formula>
  </property>
</property-set>
